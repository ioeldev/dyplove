{
	"expo": {
		"name": "dyplove",
		"slug": "dyplove",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/images/icon.png",
		"scheme": "dyplove",
		"newArchEnabled": true,
		"splash": {
			"image": "./src/assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"extra": {
			"supportsRTL": true,
			"router": {},
			"eas": {
				"projectId": "18da22d1-a89d-477b-bde9-d2ff376a9239"
			}
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"associatedDomains": ["applinks:dyplove.com", "applinks:dyplove.com"],
			"supportsTablet": true,
			"bundleIdentifier": "com.dyplove.app",
			"infoPlist": {
				"ExpoLocalization_supportsRTL": true,
				"ITSAppUsesNonExemptEncryption": false
			},
			"appleTeamId": "9345J8B4WZ"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.dyplove.app"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			"react-native-edge-to-edge",
			"expo-sqlite",
			"expo-localization"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
